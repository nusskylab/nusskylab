<table class="table table-hover">
  <thead>
  <tr><th>User Name</th><th>User Email</th><th>Actions</th></tr>
  </thead>
  <tbody>
  <% locals[:admins].each do |admin| %>
      <tr>
        <td><%= admin.user.user_name %></td>
        <td><%= admin.user.email %></td>
        <td>
          <a class="btn btn-default btn-xs" href="<%=  admin_path(admin.id) %>">View</a>
          <% if not (current_user_admin?.id == admin.id) %>
              <%= link_to 'Delete', admin_path(admin.id), method: :delete, class: 'btn btn-danger btn-xs',
                data: { confirm: 'Are you sure you want to delete this admin?' } %>
          <% end %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>
